{
  "private": true,
  "scripts": {
    "eleventy": "eleventy --serve",
    "html/eleventy": "eleventy",
    "clean/css": "rimraf dist/assets/css && mkdir -p dist/assets/css",
    "clean/js": "rimraf dist/js && mkdir -p dist/js",
    "clean/images": "rimraf dist/assets/images && midir -p dist/assets/images",
    "css/sass": "node-sass src/assets/scss/style.scss -o dist/assets/css --output-style expanded --source-map dist/assets/css",
    "css/postcss": "postcss dist/assets/css/style.css -o dist/assets/css/style.min.css",
    "css": "npm run clean/css && npm-run-all -p css/*",
    "js/rollup": "rollup src/assets/js/index.js -c=rollup.config.js",
    "js/uglifyjs": "uglifyjs dir/assets/js/index.js -o dir/assets/js/index.min.js",
    "js": "npm run clean/js && npm-run-all -s js/*",
    "images/imagemin": "node imagemin.js",
    "images": "npm run clean/images && npm-run-all -s images/*",
    "watch/server": "browser-sync start -s dist -f 'src, **/*.html, !node_modules/**/*'",
    "watch/css": "watch 'npm run css' ./src/assets/scss",
    "watch/js": "watch 'npm run js' ./src/assets/js",
    "watch/images": "onchange 'src/assets/images' -e '**/*.DS_Store' -- npm run images",
    "watch": "npm-run-all -p watch/*"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "android >= 4.4.4",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "backstopjs": "^4.4.2",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.7.2",
    "watch": "^1.0.2"
  }
}
